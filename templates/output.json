{
  "metadata": {
    "timestamp": "YYYY-MM-DDTHH:MM:SSZ",
    "hostname": "hostname",
    "platform": "macOS|Ubuntu",
    "architecture": "arm64|x86_64|amd64",
    "collection_version": "1.0"
  },
  "system_prompt": {
    "global_file": {
      "exists": true,
