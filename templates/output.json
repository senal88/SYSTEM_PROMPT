    "cursor_settings": {
      "exists": true,
      "system_prompt_enabled": true,
      "system_prompt_path": "/path/to/system_prompt_global.txt"
    }
  },
  "cursor_ide": {
    "installed": true,
    "version": "x.x.x",
    "path": "/path/to/cursor",
    "settings": {
      "exists": true,
      "system_prompt_enabled": true,
      "system_prompt_path": "/path/to/system_prompt_global.txt",
      "chat_model": "claude-3-5-sonnet"
    },
    "cursor_rules": {
      "exists": true,
      "path": "~/.cursorrules",
      "checksum": "sha256_checksum"
    },
    "extensions": {
      "count": 0,
      "directory": "/path/to/extensions"
    }
  },
  "api_keys": {
    "OPENAI_API_KEY": {
      "configured": true,
      "prefix": "sk-xxxxx...",
