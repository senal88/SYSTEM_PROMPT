      "configured": false
    },
    "env_files": {
      ".env": {
        "exists": true,
        "path": "~/.env"
      }
    }
  },
  "tools": {
    "cli_tools": {
      "ollama": {
        "installed": true,
        "version": "x.x.x",
        "path": "/usr/bin/ollama"
      },
      "python3": {
        "installed": true,
        "version": "3.x.x",
        "path": "/usr/bin/python3",
        "python_packages": {
          "openai": {
            "installed": true,
            "version": "x.x.x"
          },
          "anthropic": {
            "installed": true,
            "version": "x.x.x"
          }
        }
      }
    },
    "applications": {
      "Cursor": {
        "installed": true,
        "version": "x.x.x",
        "path": "/Applications/Cursor.app"
      }
    }
  },
  "environment": {
    "system": {
      "hostname": "hostname",
      "os_version": "x.x.x",
      "architecture": "arm64|x86_64",
      "kernel": "x.x.x"
    },
    "shell": {
      "current": "/bin/bash|/bin/zsh",
      "version": "x.x.x"
    },
    "aliases": {
      "found": true,
      "count": 0
    },
    "env_vars": {
      "PATH": "/usr/bin:/bin",
      "HOME": "/home/user",
      "EDITOR": "vim|nano"
    }
  },
  "ide_extensions": {
    "cursor": {
      "installed": true,
      "extensions_dir": "/path/to/extensions",
      "extensions": [
        {
          "name": "extension-name",
          "publisher": "publisher",
          "version": "x.x.x"
        }
